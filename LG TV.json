  "rules": {
    "zr_meeting_started": [
      "lg_tv_east_wol.power.on",
      "lg_tv_south_wol.power.on",
      "lg_tv_pillar_wol.power.on"
    ],
    "zr_meeting_ended": [
      "lg_tv_east.power.off",
      "lg_tv_south.power.off",
      "lg_tv_pillar.power.off"
    ]
  },
  "controls": [
    {
      "name": "TV Control",
      "type": "button-group",
      "label": "TV Power",
      "buttons": [
        {
          "name": "Power On TVs (Wake on LAN)",
          "action": [
            "lg_tv_east_wol.power.on",
            "lg_tv_south_wol.power.on",
            "lg_tv_pillar_wol.power.on"
          ]
        },
        {
          "name": "Power Off TVs (RS232 over IP)",
          "action": [
            "lg_tv_east.power.off",
            "lg_tv_south.power.off",
            "lg_tv_pillar.power.off"
          ]
        }
      ]
    }
  ],
